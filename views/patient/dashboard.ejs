<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Patient Dashboard ‚Äì Medisync</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;600;700;800&display=swap" rel="stylesheet">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">
  <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
  <script src="/js/dashboardCache.js"></script>
  <style>
    :root {
      --primary: #0066ff;
      --primary-dark: #0052cc;
      --secondary: #00d4aa;
      --secondary-dark: #00a8cc;
      --accent: #ff6b6b;
      --warning: #fbbf24;
      --dark: #1a1a2e;
      --light: #f8fafc;
      --gradient-hero: linear-gradient(135deg, #0066ff 0%, #00d4aa 50%, #667eea 100%);
      --gradient-patient: linear-gradient(135deg, #0066ff 0%, #0052cc 50%, #667eea 100%);
      --gradient-success: linear-gradient(135deg, #00d4aa 0%, #00a8cc 100%);
      --gradient-warning: linear-gradient(135deg, #fbbf24 0%, #f59e0b 100%);
      --gradient-info: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
    }

    * {
      margin: 0;
      padding: 0;
      box-sizing: border-box;
    }

    body {
      font-family: 'Inter', sans-serif;
      background: var(--light);
      color: var(--dark);
      min-height: 100vh;
    }

    /* Animated Background */
    .bg-animated {
      position: fixed;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      z-index: -1;
      background: linear-gradient(-45deg, #0066ff, #00d4aa, #667eea, #764ba2);
      background-size: 400% 400%;
      animation: gradientBG 15s ease infinite;
      opacity: 0.03;
    }

    @keyframes gradientBG {
      0% { background-position: 0% 50%; }
      50% { background-position: 100% 50%; }
      100% { background-position: 0% 50%; }
    }

    /* Navbar */
    .navbar-custom {
      background: rgba(255, 255, 255, 0.95);
      backdrop-filter: blur(20px);
      box-shadow: 0 2px 30px rgba(0, 0, 0, 0.08);
      padding: 1rem 0;
      position: sticky;
      top: 0;
      z-index: 1000;
    }

    .navbar-brand {
      font-size: 1.5rem;
      font-weight: 800;
      background: var(--gradient-patient);
      -webkit-background-clip: text;
      -webkit-text-fill-color: transparent;
      background-clip: text;
      display: flex;
      align-items: center;
      gap: 0.5rem;
    }

    .user-info {
      display: flex;
      align-items: center;
      gap: 1rem;
    }

    .user-avatar {
      width: 45px;
      height: 45px;
      border-radius: 50%;
      background: var(--gradient-patient);
      display: flex;
      align-items: center;
      justify-content: center;
      color: white;
      font-weight: 700;
      font-size: 1.1rem;
    }

    .user-name {
      font-weight: 600;
      color: var(--dark);
    }

    .user-role {
      font-size: 0.85rem;
      color: #64748b;
    }

    .btn-logout {
      background: transparent;
      border: 2px solid #e2e8f0;
      color: var(--dark);
      padding: 0.5rem 1.25rem;
      border-radius: 50px;
      font-weight: 600;
      transition: all 0.3s ease;
    }

    .btn-logout:hover {
      border-color: var(--accent);
      color: var(--accent);
      background: rgba(255, 107, 107, 0.1);
    }

    /* Welcome Section */
    .welcome-section {
      background: var(--gradient-patient);
      padding: 3rem 0;
      margin-bottom: 2rem;
      position: relative;
      overflow: hidden;
    }

    .welcome-section::before {
      content: '';
      position: absolute;
      top: -50%;
      right: -10%;
      width: 400px;
      height: 400px;
      border-radius: 50%;
      background: rgba(255, 255, 255, 0.1);
    }

    .welcome-section::after {
      content: '';
      position: absolute;
      bottom: -30%;
      left: -5%;
      width: 300px;
      height: 300px;
      border-radius: 50%;
      background: rgba(255, 255, 255, 0.08);
    }

    .welcome-content {
      position: relative;
      z-index: 1;
      color: white;
    }

    .welcome-content h1 {
      font-size: 2rem;
      font-weight: 800;
      margin-bottom: 0.5rem;
    }

    .welcome-content p {
      opacity: 0.9;
      font-size: 1.1rem;
    }

    .current-date {
      background: rgba(255, 255, 255, 0.2);
      padding: 0.75rem 1.5rem;
      border-radius: 50px;
      display: inline-flex;
      align-items: center;
      gap: 0.5rem;
      font-weight: 500;
    }

    /* Stats Cards */
    .stats-grid {
      display: grid;
      grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
      gap: 1.5rem;
      margin-bottom: 2rem;
    }

    .stat-card {
      background: white;
      border-radius: 20px;
      padding: 1.75rem;
      box-shadow: 0 10px 40px rgba(0, 0, 0, 0.06);
      border: 1px solid rgba(0, 0, 0, 0.04);
      transition: all 0.3s ease;
      position: relative;
      overflow: hidden;
    }

    .stat-card::before {
      content: '';
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 4px;
    }

    .stat-card.primary::before { background: var(--gradient-patient); }
    .stat-card.success::before { background: var(--gradient-success); }
    .stat-card.warning::before { background: var(--gradient-warning); }
    .stat-card.info::before { background: var(--gradient-info); }

    .stat-card:hover {
      transform: translateY(-5px);
      box-shadow: 0 20px 50px rgba(0, 0, 0, 0.1);
    }

    .stat-icon {
      width: 55px;
      height: 55px;
      border-radius: 16px;
      display: flex;
      align-items: center;
      justify-content: center;
      font-size: 1.5rem;
      margin-bottom: 1rem;
    }

    .stat-icon.primary { background: rgba(0, 102, 255, 0.1); color: var(--primary); }
    .stat-icon.success { background: rgba(0, 212, 170, 0.1); color: var(--secondary); }
    .stat-icon.warning { background: rgba(251, 191, 36, 0.1); color: #f59e0b; }
    .stat-icon.info { background: rgba(102, 126, 234, 0.1); color: #667eea; }

    .stat-number {
      font-size: 2.25rem;
      font-weight: 800;
      color: var(--dark);
      line-height: 1;
      margin-bottom: 0.25rem;
    }

    .stat-label {
      color: #64748b;
      font-weight: 500;
      font-size: 0.95rem;
    }

    /* Quick Actions */
    .quick-actions {
      display: grid;
      grid-template-columns: repeat(auto-fit, minmax(150px, 1fr));
      gap: 1rem;
      margin-bottom: 2rem;
    }

    .quick-action-btn {
      background: white;
      border: 2px solid #e2e8f0;
      border-radius: 16px;
      padding: 1.25rem;
      text-align: center;
      text-decoration: none;
      color: var(--dark);
      transition: all 0.3s ease;
      display: flex;
      flex-direction: column;
      align-items: center;
      gap: 0.75rem;
    }

    .quick-action-btn:hover {
      border-color: var(--primary);
      color: var(--primary);
      transform: translateY(-3px);
      box-shadow: 0 10px 30px rgba(0, 102, 255, 0.15);
    }

    .quick-action-btn.primary {
      background: var(--gradient-patient);
      border: none;
      color: white;
    }

    .quick-action-btn.primary:hover {
      color: white;
      box-shadow: 0 15px 40px rgba(0, 102, 255, 0.3);
    }

    .quick-action-btn i {
      font-size: 1.5rem;
    }

    .quick-action-btn span {
      font-weight: 600;
      font-size: 0.9rem;
    }

    /* Content Cards */
    .content-card {
      background: white;
      border-radius: 24px;
      box-shadow: 0 10px 40px rgba(0, 0, 0, 0.06);
      border: 1px solid rgba(0, 0, 0, 0.04);
      overflow: hidden;
      margin-bottom: 2rem;
    }

    .card-header-custom {
      padding: 1.5rem 2rem;
      border-bottom: 1px solid #e2e8f0;
      display: flex;
      align-items: center;
      justify-content: space-between;
    }

    .card-header-custom h3 {
      font-size: 1.25rem;
      font-weight: 700;
      display: flex;
      align-items: center;
      gap: 0.75rem;
      margin: 0;
    }

    .card-header-custom h3 i {
      color: var(--primary);
    }

    .card-body-custom {
      padding: 1.5rem 2rem;
    }

    /* Appointment Cards */
    .appointment-item {
      display: flex;
      align-items: center;
      gap: 1.25rem;
      padding: 1.25rem;
      background: #f8fafc;
      border-radius: 16px;
      margin-bottom: 1rem;
      transition: all 0.3s ease;
      border: 1px solid transparent;
    }

    .appointment-item:hover {
      background: white;
      border-color: #e2e8f0;
      box-shadow: 0 5px 20px rgba(0, 0, 0, 0.08);
    }

    .appointment-item:last-child {
      margin-bottom: 0;
    }

    .doctor-avatar {
      width: 55px;
      height: 55px;
      border-radius: 16px;
      background: var(--gradient-success);
      display: flex;
      align-items: center;
      justify-content: center;
      color: white;
      font-size: 1.25rem;
      flex-shrink: 0;
    }

    .appointment-details {
      flex: 1;
    }

    .appointment-details h5 {
      font-weight: 700;
      margin-bottom: 0.25rem;
      color: var(--dark);
    }

    .appointment-details .specialization {
      color: var(--secondary);
      font-weight: 500;
      font-size: 0.9rem;
    }

    .appointment-details .time {
      color: #64748b;
      font-size: 0.85rem;
      display: flex;
      align-items: center;
      gap: 0.5rem;
      margin-top: 0.25rem;
    }

    .status-badge {
      padding: 0.5rem 1rem;
      border-radius: 50px;
      font-size: 0.8rem;
      font-weight: 600;
      text-transform: capitalize;
    }

    .status-badge.pending {
      background: rgba(251, 191, 36, 0.15);
      color: #d97706;
    }

    .status-badge.accepted {
      background: rgba(0, 212, 170, 0.15);
      color: #059669;
    }

    .status-badge.rejected, .status-badge.cancelled {
      background: rgba(255, 107, 107, 0.15);
      color: #dc2626;
    }

    .status-badge.completed {
      background: rgba(102, 126, 234, 0.15);
      color: #667eea;
    }

    /* Prescription Cards */
    .prescription-item {
      display: flex;
      align-items: center;
      gap: 1.25rem;
      padding: 1.25rem;
      background: #f8fafc;
      border-radius: 16px;
      margin-bottom: 1rem;
      transition: all 0.3s ease;
      border: 1px solid transparent;
    }

    .prescription-item:hover {
      background: white;
      border-color: #e2e8f0;
      box-shadow: 0 5px 20px rgba(0, 0, 0, 0.08);
    }

    .prescription-item:last-child {
      margin-bottom: 0;
    }

    .medicine-icon {
      width: 55px;
      height: 55px;
      border-radius: 16px;
      background: rgba(0, 102, 255, 0.1);
      display: flex;
      align-items: center;
      justify-content: center;
      color: var(--primary);
      font-size: 1.25rem;
      flex-shrink: 0;
    }

    .prescription-details {
      flex: 1;
    }

    .prescription-details h5 {
      font-weight: 700;
      margin-bottom: 0.25rem;
      color: var(--dark);
    }

    .prescription-details .dosage {
      color: var(--primary);
      font-weight: 500;
      font-size: 0.9rem;
    }

    .prescription-details .doctor-info {
      color: #64748b;
      font-size: 0.85rem;
      margin-top: 0.25rem;
    }

    /* Empty State */
    .empty-state {
      text-align: center;
      padding: 3rem 2rem;
    }

    .empty-state i {
      font-size: 4rem;
      color: #e2e8f0;
      margin-bottom: 1rem;
    }

    .empty-state h4 {
      font-weight: 700;
      color: var(--dark);
      margin-bottom: 0.5rem;
    }

    .empty-state p {
      color: #64748b;
      margin-bottom: 1.5rem;
    }

    .empty-state .btn-action {
      background: var(--gradient-patient);
      color: white;
      border: none;
      padding: 0.75rem 2rem;
      border-radius: 50px;
      font-weight: 600;
      text-decoration: none;
      display: inline-flex;
      align-items: center;
      gap: 0.5rem;
      transition: all 0.3s ease;
    }

    .empty-state .btn-action:hover {
      transform: translateY(-3px);
      box-shadow: 0 10px 30px rgba(0, 102, 255, 0.3);
      color: white;
    }

    /* Chart Card */
    .chart-container {
      padding: 2rem;
      background: white;
      border-radius: 24px;
      box-shadow: 0 10px 40px rgba(0, 0, 0, 0.06);
      border: 1px solid rgba(0, 0, 0, 0.04);
      margin-bottom: 2rem;
    }

    .chart-header {
      display: flex;
      align-items: center;
      justify-content: space-between;
      margin-bottom: 1.5rem;
    }

    .chart-header h3 {
      font-size: 1.25rem;
      font-weight: 700;
      display: flex;
      align-items: center;
      gap: 0.75rem;
      margin: 0;
    }

    .chart-header h3 i {
      color: var(--primary);
    }

    /* Footer Actions */
    .footer-actions {
      display: grid;
      grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
      gap: 1rem;
      margin-bottom: 3rem;
    }

    .footer-action-btn {
      display: flex;
      align-items: center;
      gap: 1rem;
      padding: 1.25rem 1.5rem;
      background: white;
      border: 2px solid #e2e8f0;
      border-radius: 16px;
      text-decoration: none;
      color: var(--dark);
      font-weight: 600;
      transition: all 0.3s ease;
    }

    .footer-action-btn:hover {
      border-color: var(--primary);
      color: var(--primary);
      transform: translateY(-3px);
      box-shadow: 0 10px 30px rgba(0, 102, 255, 0.1);
    }

    .footer-action-btn i {
      width: 45px;
      height: 45px;
      border-radius: 12px;
      display: flex;
      align-items: center;
      justify-content: center;
      font-size: 1.25rem;
    }

    .footer-action-btn.prescriptions i {
      background: rgba(0, 102, 255, 0.1);
      color: var(--primary);
    }

    .footer-action-btn.history i {
      background: rgba(0, 212, 170, 0.1);
      color: var(--secondary);
    }

    .footer-action-btn.predictions i {
      background: rgba(251, 191, 36, 0.1);
      color: #f59e0b;
    }

    .footer-action-btn.intelligence i {
      background: rgba(102, 126, 234, 0.1);
      color: #667eea;
    }

    /* Responsive */
    @media (max-width: 768px) {
      .welcome-content h1 {
        font-size: 1.5rem;
      }

      .stats-grid {
        grid-template-columns: repeat(2, 1fr);
      }

      .quick-actions {
        grid-template-columns: repeat(2, 1fr);
      }

      .card-header-custom,
      .card-body-custom {
        padding: 1rem 1.25rem;
      }

      .footer-actions {
        grid-template-columns: 1fr;
      }
    }

    /* Animations */
    @keyframes fadeInUp {
      from {
        opacity: 0;
        transform: translateY(20px);
      }
      to {
        opacity: 1;
        transform: translateY(0);
      }
    }

    .animate-fade-in {
      animation: fadeInUp 0.5s ease forwards;
    }

    .animate-fade-in:nth-child(1) { animation-delay: 0.1s; }
    .animate-fade-in:nth-child(2) { animation-delay: 0.2s; }
    .animate-fade-in:nth-child(3) { animation-delay: 0.3s; }
    .animate-fade-in:nth-child(4) { animation-delay: 0.4s; }
  </style>
</head>
<body>
  <div class="bg-animated"></div>

  <!-- Navbar -->
  <nav class="navbar-custom">
    <div class="container">
      <div class="d-flex align-items-center justify-content-between">
        <a class="navbar-brand" href="/">
          <span>üè•</span> Medisync
        </a>
        <div class="user-info">
          <div class="user-avatar">
            <%= ((user && user.name) || 'P').charAt(0).toUpperCase() %>
          </div>
          <div class="d-none d-md-block">
            <div class="user-name"><%= (user && user.name) || 'Patient' %></div>
            <div class="user-role">Patient</div>
          </div>
          <form method="post" action="/auth/logout" class="d-inline">
            <button class="btn-logout" type="submit">
              <i class="fas fa-sign-out-alt me-1"></i> Logout
            </button>
          </form>
        </div>
      </div>
    </div>
  </nav>

  <!-- Welcome Section -->
  <section class="welcome-section">
    <div class="container">
      <div class="welcome-content">
        <div class="row align-items-center">
          <div class="col-md-8">
            <h1>Welcome back, <%= (user && user.name) || 'Patient' %>! üëã</h1>
            <p>Here's your health overview and upcoming appointments</p>
          </div>
          <div class="col-md-4 text-md-end mt-3 mt-md-0">
            <div class="current-date">
              <i class="fas fa-calendar-alt"></i>
              <span id="currentDate"></span>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <div class="container">
    <!-- Stats Grid -->
    <div class="stats-grid">
      <div class="stat-card primary animate-fade-in">
        <div class="stat-icon primary">
          <i class="fas fa-calendar-check"></i>
        </div>
        <div class="stat-number"><%= summary.totalAppointments %></div>
        <div class="stat-label">Total Appointments</div>
      </div>
      <div class="stat-card success animate-fade-in">
        <div class="stat-icon success">
          <i class="fas fa-prescription-bottle-medical"></i>
        </div>
        <div class="stat-number"><%= summary.totalPrescriptions %></div>
        <div class="stat-label">Total Prescriptions</div>
      </div>
      <div class="stat-card warning animate-fade-in">
        <div class="stat-icon warning">
          <i class="fas fa-heartbeat"></i>
        </div>
        <div class="stat-number"><%= upcoming ? upcoming.filter(a => a.status === 'pending').length : 0 %></div>
        <div class="stat-label">Pending Appointments</div>
      </div>
      <div class="stat-card info animate-fade-in">
        <div class="stat-icon info">
          <i class="fas fa-user-md"></i>
        </div>
        <div class="stat-number"><%= upcoming ? upcoming.filter(a => a.status === 'accepted').length : 0 %></div>
        <div class="stat-label">Confirmed</div>
      </div>
    </div>

    <!-- Quick Actions -->
    <div class="quick-actions">
      <a href="/patient/book" class="quick-action-btn primary">
        <i class="fas fa-calendar-plus"></i>
        <span>Book Appointment</span>
      </a>
      <a href="/health-intelligence/dashboard" class="quick-action-btn">
        <i class="fas fa-brain"></i>
        <span>Health Intelligence</span>
      </a>
      <a href="/advanced/chat-dashboard" class="quick-action-btn">
        <i class="fas fa-comments"></i>
        <span>Chat with Doctor</span>
      </a>
      <a href="/patient/prediction" class="quick-action-btn">
        <i class="fas fa-chart-line"></i>
        <span>Health Predictions</span>
      </a>
    </div>

    <div class="row">
      <!-- Appointments -->
      <div class="col-lg-6">
        <div class="content-card">
          <div class="card-header-custom">
            <h3><i class="fas fa-calendar-alt"></i> Your Appointments</h3>
            <a href="/patient/book" class="btn btn-sm" style="background: var(--gradient-patient); color: white; border-radius: 50px; padding: 0.5rem 1rem; font-weight: 600;">
              <i class="fas fa-plus me-1"></i> New
            </a>
          </div>
          <div class="card-body-custom">
            <% if (upcoming && upcoming.length > 0) { %>
              <% upcoming.forEach(a => { %>
                <div class="appointment-item">
                  <div class="doctor-avatar">
                    <i class="fas fa-user-md"></i>
                  </div>
                  <div class="appointment-details">
                    <h5>Dr. <%= a.doctor_name %></h5>
                    <div class="specialization"><%= a.specialization %></div>
                    <div class="time">
                      <i class="fas fa-clock"></i>
                      <%= new Date(a.start_time).toLocaleString() %>
                    </div>
                  </div>
                  <span class="status-badge <%= a.status %>"><%= a.status %></span>
                </div>
              <% }) %>
            <% } else { %>
              <div class="empty-state">
                <i class="fas fa-calendar-times"></i>
                <h4>No Appointments</h4>
                <p>You haven't booked any appointments yet</p>
                <a href="/patient/book" class="btn-action">
                  <i class="fas fa-calendar-plus"></i> Book Now
                </a>
              </div>
            <% } %>
          </div>
        </div>
      </div>

      <!-- Prescriptions -->
      <div class="col-lg-6">
        <div class="content-card">
          <div class="card-header-custom">
            <h3><i class="fas fa-pills"></i> Recent Prescriptions</h3>
            <a href="/patient/prescriptions" class="btn btn-sm" style="background: rgba(0, 102, 255, 0.1); color: var(--primary); border-radius: 50px; padding: 0.5rem 1rem; font-weight: 600;">
              View All <i class="fas fa-arrow-right ms-1"></i>
            </a>
          </div>
          <div class="card-body-custom">
            <% if (prescriptions && prescriptions.length > 0) { %>
              <% prescriptions.slice(0, 5).forEach(p => { %>
                <div class="prescription-item">
                  <div class="medicine-icon">
                    <i class="fas fa-capsules"></i>
                  </div>
                  <div class="prescription-details">
                    <h5><%= p.medicine_name %></h5>
                    <div class="dosage"><%= p.dosage %> ‚Ä¢ <%= p.duration %></div>
                    <div class="doctor-info">
                      <i class="fas fa-user-md me-1"></i> Dr. <%= p.doctor_name %>
                    </div>
                  </div>
                </div>
              <% }) %>
            <% } else { %>
              <div class="empty-state">
                <i class="fas fa-prescription-bottle"></i>
                <h4>No Prescriptions</h4>
                <p>You don't have any prescriptions yet</p>
              </div>
            <% } %>
          </div>
        </div>
      </div>
    </div>

    <!-- Medicine Chart -->
    <div class="chart-container">
      <div class="chart-header">
        <h3><i class="fas fa-chart-bar"></i> Medicine Usage Analytics</h3>
      </div>
      <div style="max-width: 800px; margin: 0 auto;">
        <canvas id="medChart"></canvas>
      </div>
      <% if (!prescriptions || prescriptions.length === 0) { %>
        <div class="empty-state">
          <i class="fas fa-chart-area"></i>
          <h4>No Data Available</h4>
          <p>Medicine analytics will appear here once you have prescriptions</p>
        </div>
      <% } %>
    </div>

    <!-- Footer Actions -->
    <div class="footer-actions">
      <a href="/patient/prescriptions" class="footer-action-btn prescriptions">
        <i class="fas fa-file-prescription"></i>
        <span>View All Prescriptions</span>
      </a>
      <a href="/patient/history" class="footer-action-btn history">
        <i class="fas fa-history"></i>
        <span>Disease History</span>
      </a>
      <a href="/patient/prediction" class="footer-action-btn predictions">
        <i class="fas fa-brain"></i>
        <span>Health Predictions</span>
      </a>
      <a href="/health-intelligence/dashboard" class="footer-action-btn intelligence">
        <i class="fas fa-analytics"></i>
        <span>Health Intelligence</span>
      </a>
    </div>
  </div>

  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>
  <script>
    // Set current date
    document.getElementById('currentDate').textContent = new Date().toLocaleDateString('en-US', {
      weekday: 'long',
      year: 'numeric',
      month: 'long',
      day: 'numeric'
    });

    // Medicine Chart
    const meds = {};
    <% prescriptions.forEach(p => { %>
      meds['<%= p.medicine_name %>'] = (meds['<%= p.medicine_name %>'] || 0) + 1;
    <% }) %>
    const labels = Object.keys(meds);
    const data = Object.values(meds);
    
    if (labels.length > 0) {
      const ctx = document.getElementById('medChart').getContext('2d');
      const gradient = ctx.createLinearGradient(0, 0, 0, 400);
      gradient.addColorStop(0, 'rgba(0, 102, 255, 0.8)');
      gradient.addColorStop(1, 'rgba(0, 212, 170, 0.8)');

      new Chart(ctx, {
        type: 'bar',
        data: {
          labels: labels,
          datasets: [{
            label: 'Times Prescribed',
            data: data,
            backgroundColor: gradient,
            borderRadius: 8,
            borderSkipped: false,
          }]
        },
        options: {
          responsive: true,
          plugins: {
            legend: {
              display: false
            }
          },
          scales: {
            y: {
              beginAtZero: true,
              grid: {
                color: 'rgba(0, 0, 0, 0.05)'
              },
              ticks: {
                font: {
                  family: 'Inter'
                }
              }
            },
            x: {
              grid: {
                display: false
              },
              ticks: {
                font: {
                  family: 'Inter'
                }
              }
            }
          }
        }
      });
    }

    // Initialize dashboard cache on page load
    document.addEventListener('DOMContentLoaded', function() {
      // Initialize cache UI with controls
      if (typeof DashboardCache !== 'undefined') {
        DashboardCache.initUI();
        console.log('[Patient Dashboard] Cache initialized');
        console.log('[Patient Dashboard] Cache Stats:', DashboardCache.getStats());
        
        DashboardCache.set('patient_dashboard_summary', {
          timestamp: new Date().toISOString(),
          page: 'patient_dashboard'
        }, DashboardCache.CONFIG.PATIENT_TTL);
      }
    });
  </script>
</body>
</html>